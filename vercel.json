{
  "version": 2,
  "devCommand": "npm run dev",
  "builds": [
   {
    "src": "src/server-with-mysql.js",
    "use": "@vercel/node",
    "config": { "includeFiles": ["src/dist/**"] }
   },
   {
    "src": "src/public",
    "use": "@vercel/static"
   }
  ],
  "routes": [
   {
    "src": "/(.*)",
    "dest": "/src/server-with-mysql.js"
   }
  ]
 }